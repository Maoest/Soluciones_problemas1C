n = int(input(" ingrese el numero del cual quiere que se su tabla de multiplicar : "))
k = int(input("ingrese el numero con el cual quiere comenzar la tabla : "))
s = int(input("ingrese el numero el cual es el limite de la tabla: "))

def tabla_multiplicacion(n):
    for i in range (k,s+1):
        print(i, "*", n, "=", i*n)
tabla_multiplicacion(n)
